  justify-content: space-between;
}
.section-multi-component .mc-catering-firstRow > div {
  width: 500px;
}
.section-multi-component .mc-cateringTitle {
  color: #D90707;
  font-weight: 600;
  letter-spacing: 0;
  font-size: 30px;
}
.section-multi-component .mc-cateringDesc p {
  margin-bottom: 0;
}
.section-multi-component .mc-desc a {
  color: #D90707;
}
.section-multi-component .mc-imgBlock img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
}
.section-multi-component .mc-socialBlock {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 50px;
}
.section-multi-component .mc-socialBlock > * {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 7px;
}
.section-multi-component .mc-socialBlock img {
  width: 30px;
  height: 30px;
  display: block;
}
.section-multi-component .mc-socialBlock span {
  font-weight: 600;
  font-size: 20px;
  color: #222;
  letter-spacing: 0;
  line-height: 1;
}
@media (max-width: 749px) {
  .section-multi-component .mc-title {
    line-height: 1.2;
    font-size: 25px;
    padding: 15px 20px;
  }
  .section-multi-component .mc-title p {
    margin: 0;
  }
  .section-multi-component .mc-btnsBlock {
    flex-wrap: wrap;
  }
  .section-multi-component a.custom-btn {
    min-width: 180px;
  }
  .section-multi-component .mc-catering-firstRow {
    flex-wrap: wrap;
  }
  .section-multi-component .mc-catering-firstRow > div {
    width: 90%;
  }
  .section-multi-component .mc-cateringTitle {
    font-size: 22px;
  }
  .section-multi-component .mc-socialBlock {
    flex-direction: column;
    gap: 20px;
  }
}


/* ----------- Images Section Styling ------------ */
.section-images .section-images-wrapper {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.section-images .image-individual {
  margin-bottom: 20px;
}
.section-images .image-individual img {
  width: 100%;
  height: 100%;
  object-fit: contain;
  object-position: center;
}
.section-images .image-individual img.img-cover {
  object-fit: cover;
}
.section-images .image-individual img.img-gap {
  width: calc(100% - 20px);
  margin: 0 10px;
}
.section-images .swiper-button {
  color: #fff;
}



/* ----------- Contact Form Styling ------------ */
.custom-form .half-width-field {
  display: flex;
  gap: 20px;
}
.custom-form .field__input,
.custom-form .field__label {
  font-size: 13px;
  letter-spacing: 0;
  font-family: inherit;
}
.custom-form .field__label {
  top: 50%;
  transform: translateY(-50%);
}
.custom-form .field__input[type=date]:not(:placeholder-shown)~.field__label {
  top: 20%;
}
.custom-form .field__input:focus~.field__label {
  transform: unset;
}
.custom-form input[type=date].field__input {
  padding: 1.5rem;
}
.custom-form select.field__input {
  padding: 1.5rem;
  line-height: 1;
  position: relative;
}
