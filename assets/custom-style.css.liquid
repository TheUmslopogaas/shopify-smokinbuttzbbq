/* ----------- Multicolumn Custom Styling ------------ */
.section-multicol-custom .col-single {
  display: block;
}
.section-multicol-custom .multicol-custom-wrapper {
  display: flex;
  flex-wrap: wrap;
  row-gap: 40px;
}
.section-multicol-custom .col-img img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.section-multicol-custom .col-single-wrap {
  padding: 0 10px;
  display: flex;
  flex-direction: column;
  text-align: center;
}
.section-multicol-custom .col-title {
  font-weight: 600;
  letter-spacing: 0;
  padding: 0 2px;
  line-height: 1.4;
  color: #222;
}
.section-multicol-custom .col-img {
  height: 200px;
  margin-bottom: 10px;
}
.section-multicol-custom .col-date {
  margin-top: 5px;
  letter-spacing: 0;
  font-size: 14px;
  color: #222;
}
.section-multicol-custom .col-readmore {
  color: #222;
  margin: 0;
  font-size: 12px;
  text-decoration: underline;
}


/* ----------- Multicolumn (Dawn theme default) Styling ------------ */
body .background-none .multicolumn-card__image-wrapper+.multicolumn-card__info {
  padding: 1rem;
}
body .multicolumn .inline-richtext {
  line-height: 1;
  font-size: 16px;
  letter-spacing: 0px;
  font-weight: 600;
}
@media (min-width: 750px) {
  .background-none .multicolumn-card__image-wrapper, 
  .background-none .multicolumn-card__info {
    margin-left: 1.5rem;
    margin-right: 1.5rem;
  }
}


/* ----------- Vertical Timeline Styling ------------ */
.section-timeline .row-individual {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  position: relative;
}
.section-timeline .row-individual > * {
  width: calc(50% - 50px);
}
.section-timeline .row-individual::after {
  content: '';
  position: absolute;
  width: 6px;
  background: #222;
  height: 100%;
  left: 50%;
  transform: translateX(-50%);
}
.section-timeline .row-individual::before {
  content: '';
  position: absolute;
  width: 20px;
  height: 20px;
  border-radius: 50px;
  background: red;
  z-index: 1;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.section-timeline .row-image {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-right: 50px;
}
.section-timeline .row-image img {
  height: 230px;
  width: 230px;
  object-fit: cover;
  border-radius: 100rem;
  border: 10px solid #d90707;
}
.section-timeline .row-title {
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-left: 50px;
  font-weight: 600;
  color: #222;
  font-size: 30px;
  letter-spacing: 0;
}
.section-timeline .row-individual.img-right {
  flex-direction: row-reverse;
}
.section-timeline .img-right .row-image {
  justify-content: flex-start;
  margin-right: 0;
  margin-left: 50px;
}
.section-timeline .img-right .row-title {
  justify-content: flex-end;
  margin-left: 0;
  margin-right: 50px;
  text-align: right;
}
.section-timeline .row-title p {
  margin: 10px;
}
@media (max-width: 749px) {
  .section-timeline .row-individual {
    flex-direction: column;
    margin-bottom: 150px;
  }
  .section-timeline .row-individual.img-right > *,
  .section-timeline .row-individual > * {
    width: 100%;
    margin: 0;
    justify-content: center;
    text-align: center;
  }
  .section-timeline .row-individual:before {
    top: unset;
    bottom: 0;
    transform: translate(-50%, 65px);
  }
  .section-timeline .row-individual:after {
    bottom: 0;
    transform: translate(-50%, 100%);
    height: 120px;
  }
  .section-timeline .row-individual:last-child {
    margin: 0;
  }
  .section-timeline .row-individual:last-child::before,
  .section-timeline .row-individual:last-child::after {
    content: unset;
  }
  .section-timeline .row-image img {
    border: 5px solid #d90707;
  }
}


/* ----------- Recipe List Styling ------------ */
.section-recipe-list .recipelist-wrapper {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  row-gap: 20px;
  margin-top: 50px;
}
.section-recipe-list .recipe-individual {
  width: 50%;
  padding: 10px 20px;
  max-width: 450px;
}
.section-recipe-list .recipe-title {
  font-weight: 600;
  letter-spacing: 0;
  color: #222;
  font-size: 22px;
  line-height: 1.4;
}
.section-recipe-list .recipe-desc {
  line-height: 1.4;
  color: #222;
}
@media (max-width: 749px) {
  .section-recipe-list .recipe-individual {
    width: 95%;
    margin: 0 auto;
  }
}


/* ----------- Recipe List Tab Styling ------------ */
.section-recipelist-tab .recipelist-tab-wrap {
  display: flex;
  gap: 50px;
  justify-content: center;
}
