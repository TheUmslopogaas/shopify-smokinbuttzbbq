  justify-content: center;
}
.section-multi-component .mc-list {
  flex-grow: 1;
}
.section-multi-component .mc-list-title {
  font-weight: 600;
  font-size: 18px;
  color: #d90707;
  letter-spacing: 0;
  text-decoration: underline;
}
.section-multi-component .mc-list-desc p {
  margin: 0;
}
.section-multi-component .mc-list-desc {
  line-height: 1.4;
  color: #111;
}
.section-multi-component .mc-block {
