@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap');
/* ------------ Importing Fonts ------------  */
@font-face {font-family: 'c-font-light';src: url("{{ 'c-font-light.ttf' | asset_url }}")}
@font-face {font-family: 'c-font-regular';src: url("{{ 'c-font-regular.ttf' | asset_url }}")}
@font-face {font-family: 'c-font-medium';src: url("{{ 'c-font-medium.ttf' | asset_url }}")}
@font-face {font-family: 'c-font-semibold';src: url("{{ 'c-font-semibold.ttf' | asset_url }}")}
@font-face {font-family: 'c-font-bold';src: url("{{ 'c-font-bold.ttf' | asset_url }}")}



/* ------------------ Dawn Theme Style Overwrite ------------------ */
a,
.header__menu-item span,
a:hover,
.header__menu-item:hover span,
.header__menu-item span:hover,
body .list-menu__item.list-menu__item--active,
details[open]>.header__menu-item,
body .underline-links-hover:hover a,
.copyright__content a:hover,
body .footer-block__details-content .list-menu__item--link:hover,
body .header__active-menu-item {
  text-decoration: none;
}
a:empty, ul:empty, dl:empty, div:empty, section:empty, article:empty, p:empty, h1:empty, h2:empty, h3:empty, h4:empty, h5:empty, h6:empty {
  display: unset;
}


/* ------------------ Global Styling ------------------ */
html {scroll-behavior: smooth;}
a.custom-btn {
  display: inline-block;
  min-width: 150px;
  font-weight: 500;
  color: #fff;
  background-color: #D90707;
  letter-spacing: 0;
  border: 2px solid #D90707;
  padding: 8px 10px;
  margin: 10px 0;
  text-align: center;
  transition: 0.2s;
}
a.custom-btn:hover {
  background-color: rgb(143,152,226);
  border-color: rgb(143,152,226);
}
a.custom-btn.custom-btn-outlined {
  color: #D90707;  
  background-color: #fff;
  border: 2px solid #D90707;
}
.title {
  font-family: 'c-font-semibold';
  text-align: center;
  margin-bottom: 50px;
  font-size: 50px;
  letter-spacing: 2px;
  text-shadow: #c8c8c8 1px 1px 0px, #b4b4b4 0px 2px 0px, #a0a0a0 0px 3px 0px, rgba(140, 140, 140, 0.498039) 0px 4px 0px, #787878 0px 0px 0px, rgba(0, 0, 0, 0.498039) 0px 5px 10px;
}
.full-width-title {
  font-size: 30px;
  color: #fff;
  background-color: #D90707;
  text-align: center;
}
.full-width-title > * {
  font-size: 30px;
}
.text-left {text-align: left;}
.text-center {text-align: center;}
.text-right {text-align: right;}
@media (max-width: 749px) {
  a.custom-btn {
    font-size: 14px;
    min-width: 120px;
    padding: 6px 8px;
  }
  .title {
    font-size: 30px;
  }
}


/* ---------------- Announcement Bar Styling ---------------- */
.announcement-bar__link:hover {
  color: unset;
  background-color: unset;
}
.announcement-bar .page-width {
  display: flex;
  justify-content: center;
  gap: 40px;
}
.announcement-bar__link .announcement-bar__message {
  font-size: 16px;
  font-weight: 600;
  letter-spacing: 0.5px;
  color: #fff;
  font-style: italic;
  padding: 0 10px;
  transition: 0.3s;
}
.announcement-bar__link .announcement-bar__message:hover {
  color: #D90707;
}
@media (max-width: 749px) {
  .announcement-bar__link .announcement-bar__message {
    font-size: 13px;
    min-width: fit-content;
  }
  .announcement-bar .page-width {
    gap: 10px;
  }
}


/* ----------------- Header Styling ----------------- */
body .list-menu__item,
body .menu-drawer__close-button {
  font-weight: 600;
  color: #fff;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  font-size: 16px;
  transition: 0.2s;
}
body .list-menu__item:hover,
body .list-menu__item:hover a {
  color: rgb(253,220,13);
} 
.header__submenu.list-menu {
  padding: 0;
  background-color: #151515;
}
.header__menu-item a {
  color: #fff;
  transition: 0.2s;
}
.header__icon--cart .icon {
  height: 3rem;
  width: 3rem;
}
.header__icon.header__icon--menu {
  height: 3.5rem;
  width: 3.5rem;
}
.header__icon .icon.icon-hamburger {
  height: 3rem;
  width: 3rem;
}
body .menu-drawer__menu-item a {
  color: #fff;
}
@media (min-width: 990px) {
  body .header, body .header--middle-left {
    grid-template-columns: auto 1fr auto;
  }
  body .header__inline-menu {
    text-align: center;
  }
  body .header__menu-item .icon-caret {
    display: none;
  }
}
@media (min-width: 750px) {
  body .header {padding: 0;}
}
@media (max-width: 749px) {
  .header__heading-logo {
    max-height: 60px;
    object-fit: contain;
  }
  body .menu-drawer__navigation {
    padding-top: 2rem;
  }
}


/* --------------- Banner Slider Styling ---------------- */
.section-banner-slider .bs-slideItem {
  min-height: 500px;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
.section-banner-slider .bs-slideItem::after {
  content: '';
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}
.section-banner-slider .page-width {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  max-width: 800px;
  text-align: center;
  position: relative;
  z-index: 9;
}
.section-banner-slider .bs-pretitle {
  font-family: 'c-font-semibold';
  font-size: 50px;
  color: #D90707;
  letter-spacing: 4px;
  line-height: 1.2;
  margin-bottom: 20px;
  text-shadow: #c8c8c8 1px 1px 0px, #b4b4b4 0px 2px 0px, #a0a0a0 0px 3px 0px, rgba(140, 140, 140, 0.498039) 0px 4px 0px, #787878 0px 0px 0px, rgba(0, 0, 0, 0.498039) 0px 5px 10px;
}
.section-banner-slider .bs-title {
  font-family: 'c-font-bold';
  font-size: 80px;
  color: #FFF;
  letter-spacing: 6px;
  line-height: 1.2;
  margin-bottom: 30px;
  text-shadow: #c8c8c8 1px 1px 0px, #b4b4b4 0px 2px 0px, #a0a0a0 0px 3px 0px, rgba(140, 140, 140, 0.498039) 0px 4px 0px, #787878 0px 0px 0px, rgba(0, 0, 0, 0.498039) 0px 5px 10px;
}
.section-banner-slider .bs-desc {
  font-size: 20px;
  font-family: 'c-font-medium';
}
.section-banner-slider .bs-desc p {
  margin-top: 0;
}
.section-banner-slider .bs-btns {
  display: flex;
  gap: 20px;
}
.section-banner-slider .swiper-button {
  color: #fff;
}
.section-banner-slider a.custom-btn {
  box-shadow: 0 0 10px rgba(255,255,255,0.3);
}
@media (max-width: 749px) {
  .section-banner-slider .bs-pretitle {
    font-size: 30px;
    letter-spacing: 2px;
  }
  .section-banner-slider .bs-title {
    font-size: 50px;
  }
  .section-banner-slider .bs-slideItem {
    min-height: 400px;
  }
  .section-banner-slider .bs-desc {
    font-size: 16px;
  }
}


/* ----------- Image With Text Styling (Dawn Default) ------------ */
.section-image-with-text .image-with-text__media {
  border: none;
}
.section-image-with-text .image-with-text__text.shrinked {
  line-height: 1.2;
  font-size: 15px;
}
.section-image-with-text .image-with-text__text {
  color: #121212;
  line-height: 1.5;
}
.section-image-with-text .image-with-text__text a {
  text-decoration: underline;
}
.section-image-with-text .section-imgwtxt-btns {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 20px;
}

@media (min-width: 990px) {
  body .image-with-text__content {
    padding: 4rem;
  }
} 
@media (max-width: 749px) {
  body .image-with-text--overlap .image-with-text__content {
    margin: 1rem auto 0;
    padding: 1rem;
  }
  body .image-with-text__content {
    padding: 3rem 1rem;
  }
}


/* ----------- Feature Collection Styling (Dawn Default) ------------ */
.section-featured-collection .card__inner {
  flex-grow: 1;
}
.section-featured-collection .card__content {
  flex-grow: unset;
}
.section-featured-collection .card__heading, 
.section-featured-collection .card-information .price {
  text-align: center;
  font-size: 15px;
}
.section-featured-collection a.custom-btn,
.section-featured-collection .collection__view-all a:not(.link) {
  margin-top: 30px;
}


/* ----------- Accordion Styling ------------ */
.section-accordion .accordion-wrapper {
  display: flex;
  gap: 20px;
  flex-wrap: wrap;
  align-items: flex-start;
}
.section-accordion .accordion-individual {
  cursor: pointer;
  background: red;
  width: calc(25% - 20px);
  padding: 10px;
}
.section-accordion .accordion-title {
  font-size: 40px;
  letter-spacing: 0;
  text-align: center;
}
.section-accordion .accordion-question {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 18px;
  font-weight: 500;
  color: #fff;
  letter-spacing: 0;
  padding: 0 15px;
}
.section-accordion .accordion-question span {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 18px;
  width: 18px;
  border-radius: 50%;
  background-color: #FFF;
  color: #000;
}
.section-accordion .accordion-question span i {
  font-size: 8px;
}
.section-accordion .accordion-answer {
  max-height: 0;
  overflow: hidden;
  transition: 0.3s ease-out;
  padding: 0 15px;
  color: #fff;
  font-size: 14px;
  letter-spacing: 0;
}
.section-accordion .accordion-answer p {
  margin-top: 0;
}
.section-accordion .accordion-answer a {
  color: #121212;
}
@media (max-width: 749px) {
  .section-accordion .accordion-title {
    font-size: 35px;
  }
}


/* ----------- Address List Styling ------------ */
.section-address-list .page-width {
  display: flex;
  flex-wrap: wrap;
  gap: 20px;
}
.section-address-list .al-block {
  width: calc(50% - 10px);
}
.section-address-list .al-textBlock .title {
  font-size: 40px;
  margin: 0 0 30px;
}
.section-address-list .al-textBlock .desc {
  text-align: center;
  line-height: 1.2;
  color: #888;
}
.section-address-list .al-textBlock {
  padding: 0 20px 20px;
}
.section-address-list .al-addBlock .address-description {
  line-height: 1.5;
  font-size: 14px;
  max-width: 300px;
}
.section-address-list .al-addBlock .address-description ul {
  padding-left: 20px;
}
.section-address-list .al-addBlock-wrap {
  background: #ebebeb;
  padding: 30px;
  border: 2px solid #505050;
  border-radius: 5px;
}
.section-address-list .al-addBlock p {
  margin: 0 0 10px 0;
  overflow: hidden;
  display: block;
}
.section-address-list .al-imgBlock img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.section-address-list .al-addBlock .location-address {
  font-weight: 600;
  letter-spacing: 0;
  font-size: 25px;
  margin-bottom: 10px;
  color: #303030;
  line-height: 1;
}
.section-address-list .al-addBlock .location-name {
  letter-spacing: 0;
  font-size: 18px;
  font-weight: 500;
  line-height: 1;
  margin-bottom: 20px;
}
@media (max-width: 749px) {
  .section-address-list .al-block {
    width: 100%;
  }
}



/* ----------- Multi Component Styling ------------ */
.section-multi-component .mc-desc {
  color: #111;
  line-height: 1.4;
}
.section-multi-component .mc-list-number {
  color: #D90707;
  border: 4px solid #D90707;
  line-height: 1;
  border-radius: 200px;
